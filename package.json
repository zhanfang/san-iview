    "babel-runtime": "^6.26.0",
    "chai": "^3.5.0",
    "cross-env": "^3.2.4",
    "css-loader": "^0.23.1",
    "eslint": "^3.19.0",
    "eslint-plugin-vue": "^2.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
